{
  "name": "vuex-plugin-firemodel",
  "version": "1.0.0-beta.97",
  "description": "A Vuex plugin which reponds to all FireModel actions as well as providing mutations which can be imported by your state tree.",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/esnext/index.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "clean:nuclear": "rimraf lib dist node_modules && yarn cache clean && rm yarn.lock && yarn && yarn upgrade",
    "test": "yarn do test",
    "docs": "vuepress dev docs",
    "build": "yarn tsc -P tsconfig.esnext.json && yarn tsc -P tsconfig.cjs.json",
    "watch": "yarn tsc --watch -P tsconfig.json",
    "build:docs": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forest-fire/vuex-plugin-firemodel.git"
  },
  "keywords": [
    "firebase",
    "firemodel",
    "vuejs",
    "vuex",
    "state-management"
  ],
  "author": "Ken Snyder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forest-fire/vuex-plugin-firemodel/issues"
  },
  "homepage": "https://vuex.firemodel.info",
  "dependencies": {
    "@firebase/auth-types": "^0.10.0",
    "abstracted-client": "^0.38.1",
    "common-types": "^1.14.2",
    "fast-copy": "^2.0.4",
    "fast-equals": "^2.0.0",
    "firemodel": "^0.38.0",
    "js-cookie": "^2.2.1",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "stacktrace-js": "^2.0.2",
    "typed-conversions": "^0.10.2",
    "vuex": "^3.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/js-cookie": "^2.2.4",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/mocha": "^5.2.7",
    "@vuepress/plugin-back-to-top": "^1.4.1",
    "@vuepress/plugin-last-updated": "^1.4.1",
    "@vuepress/plugin-medium-zoom": "^1.4.1",
    "@vuepress/plugin-pwa": "^1.4.1",
    "async-shelljs": "^0.1.4",
    "chai": "^4.2.0",
    "do-devops": "^0.3.6",
    "eslint-plugin-vue": "^6.2.2",
    "fake-indexeddb": "^3.0.0",
    "firemock": "^0.38.4",
    "mocha": "^6.2.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vuepress": "^1.4.1",
    "vuepress-plugin-container": "^2.1.3"
  }
